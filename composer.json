{
    "name": "tonning/commits",
    "description": "A Laravel package to get latest git commit messages.",
    "keywords": [
        "tonning",
        "commits"
    ],
    "homepage": "https://github.com/tonning/commits",
    "license": "MIT",
    "authors": [
        {
            "name": "Kristoffer Tonning",
            "email": "tonning@gmail.com",
            "homepage": "https://kristoffertonning.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Tonning\\Commits\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tonning\\Commits\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
